{
  "manifest_version": 3,
  "name": "OneDub",
  "version": "1.3.2",
  "description": "Play movie on your favourite website with ai dubbing applied to it",
  "permissions": [
    "activeTab",
    "tabs",
    "offscreen",
    "alarms",
    "storage",
    "identity",
    "identity.email",
    "scripting"
  ],
  "key": "",
  "oauth2": {
    "client_id": "75200943505-igj5qtoj017a42ocutobucdtvums6u5v.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": ["https://*/", "http://*/"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/mic-active-filled16.png",
      "48": "icons/mic-active-filled48.png",
      "128": "icons/mic-active-filled128.png"
    }
  },
  "icons": {
    "16": "icons/mic-active-filled16.png",
    "48": "icons/mic-active-filled48.png",
    "128": "icons/mic-active-filled128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/*", "content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
